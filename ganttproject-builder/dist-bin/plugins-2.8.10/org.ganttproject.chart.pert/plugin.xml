<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.ganttproject.chart.pert"
   name="Plug-in Manager"
   version="1.0.0"
   provider-name="">

   <runtime>
      <library name="org.ganttproject.chart.pert.jar"/>
      <library name="resources/">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="biz.ganttproject.core"/>
      <import plugin="net.sourceforge.ganttproject"/>
      <import plugin="org.eclipse.core.runtime"/>
   </requires>
   <extension
           name="Pert Plug-in"
           version="1.0.0"
           point="net.sourceforge.ganttproject.chart">
         <chart class="org.ganttproject.chart.pert.ActivityOnNodePertChart"/>
   </extension>
   <extension
           name="Planner Plug-in"
           version="1.0.0"
           point="net.sourceforge.ganttproject.chart">
      <chart class="org.ganttproject.chart.planner.PlannerPanel"/>
   </extension>
   <extension
           name="Burndown Chart Plug-in"
           version="1.0.0"
           point="net.sourceforge.ganttproject.chart">
      <chart class="org.ganttproject.chart.burndownchart.BurndownChartPanel"/>
   </extension>
</plugin>
